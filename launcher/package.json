{
  "name": "stereum-launcher",
  "version": "2.3.2",
  "private": true,
  "description": "Stereum Ethereum Node Setup Launcher",
  "author": {
    "name": "Stereum Launcher Team",
    "email": "office@stereum.com",
    "url": "https://stereum.com/"
  },
  "scripts": {
    "serve": "vue-cli-service serve --port 8081",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-themes": "node-sass src/styles/themes -o public/themes",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "sed 's/^#include .nan_scriptorigin\\.h./\\/\\/ #include nan_scriptorigin.h/' ./node_modules/nan/nan.h > ./node_modules/nan/nan.h.new && mv ./node_modules/nan/nan.h.new ./node_modules/nan/nan.h && electron-builder install-app-deps",
    "postuninstall": "sed 's/^#include .nan_scriptorigin\\.h./\\/\\/ #include nan_scriptorigin.h/' ./node_modules/nan/nan.h > ./node_modules/nan/nan.h.new && mv ./node_modules/nan/nan.h.new ./node_modules/nan/nan.h && electron-builder install-app-deps",
    "test:unit": "jest '(\\.test|\\.spec)\\.js' --coverage",
    "test:int": "jest '\\.int\\.js'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "watch:css": " npx @tailwindcss/cli -i ./src/main.css -o ./public/output.css --watch",
    "lint:fix": "eslint --ext .js,.vue --ignore-path ../.gitignore --fix src",
    "stereum": "concurrently \"npm:electron:serve\" \"npm:watch:css\"",
    "backend:watch": "nodemon --watch 'src/backend' --exec 'npm run electron:serve'",
    "format": "prettier .  --write",
    "format:check": "prettier .  --check"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@sapien99/vue-cli-plugin-electron-builder-notarize": "^1.1.3",
    "@vue-hero-icons/outline": "^1.7.2",
    "@vue-hero-icons/solid": "^1.7.2",
    "@vue/cli": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.5.13",
    "animate.css": "^4.1.1",
    "axios": "^1.7.9",
    "electron-builder": "^25.1.8",
    "electron-log": "^5.3.0",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.3.9",
    "evilscan": "^1.9.1",
    "file-saver": "^2.0.5",
    "geoip-lite": "^1.4.10",
    "jszip": "^3.10.1",
    "leader-line-new": "^1.1.9",
    "lodash": "^4.17.21",
    "ping": "^0.4.4",
    "pinia": "^3.0.1",
    "preline": "^2.7.0",
    "qrcode": "^1.5.4",
    "semver": "^7.7.1",
    "ssh2": "^1.16.0",
    "swiper": "^11.2.4",
    "tailwindcss-animated": "^2.0.0",
    "vue": "^3.5.13",
    "vue-i18n": "11.1.1",
    "vue-router": "^4.5.0",
    "vue3-apexcharts": "^1.8.0",
    "vue3-carousel": "^0.14.0",
    "vue3-circle-progress": "^1.0.7",
    "vuedraggable": "^2.24.3",
    "web3": "^4.16.0",
    "xterm": "^5.3.0",
    "xterm-addon-attach": "^0.9.0",
    "xterm-addon-fit": "^0.8.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.7",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/xterm": "^3.0.0",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "@vueuse/core": "^12.7.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.2",
    "prettier": "^3.5.1",
    "tailwind-scrollbar": "^4.0.0",
    "tailwindcss": "^4.0.7",
    "vue-cli-plugin-electron-builder": "^3.0.0-alpha.4",
    "vue-loader": "^17.4.2",
