    "electron-updater": "^5.2.1",
    "evilscan": "^1.9.1",
    "file-saver": "^2.0.5",
    "geoip-lite": "^1.4.7",
    "gsap": "^3.12.7",
    "jszip": "^3.10.1",
    "leader-line-new": "^1.1.9",
    "lodash": "^4.17.21",
    "ping": "^0.4.4",
    "pinia": "^2.0.33",
    "preline": "^2.7.0",
    "qrcode": "^1.5.1",
    "semver": "^7.6.0",
