{
  "name": "stereum-launcher",
  "version": "2.0.0",
  "private": true,
  "description": "Stereum Ethereum Node Setup Launcher",
  "author": "stereum.net",
  "scripts": {
    "serve": "vue-cli-service serve --port 8081",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "build-themes": "node-sass src/styles/themes -o public/themes",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "modernizr": "modernizr -c modernizr-config.json -d node_modules/modernizr/modernizr.custom.js",
    "postinstall": "npm run modernizr && electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stereum-dev/ethereum-node.git"
  },
  "main": "background.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.15.1",
    "electron-log": "^4.4.1",
    "electron-promise-ipc": "^2.2.4",
    "electron-store": "^8.0.0",
    "js-yaml": "^4.1.0",
    "ssh2": "^1.1.0",
    "tunnel-ssh": "^4.1.4",
    "vue": "^2.6.14",
    "vue-form-wizard": "^0.8.4",
    "vue-router": "^3.5.2",
    "vue-toasted": "^1.1.28",
    "vuex": "^3.6.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@vue/cli-plugin-babel": "4.5.8",
    "@vue/cli-plugin-eslint": "4.5.8",
    "@vue/cli-plugin-router": "4.5.8",
    "@vue/cli-plugin-vuex": "4.5.8",
    "@vue/cli-service": "4.5.8",
    "babel-eslint": "10.0.3",
    "electron": "^11.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "5.0.0",
    "imports-loader": "0.8.0",
    "jest": "^27.3.1",
    "modernizr": "^3.11.7",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.6",
    "vue-template-compiler": "^2.6.14"
  },
  "license": "https://wrapbootstrap.com/help/licenses"
}
